{
  "name": "eagle-versioner",
  "version": "1.0.0",
  "description": "From the creation of special commits, calculates the semantic version based on the Git branch and commit history.",
  "engines": {
    "npm": ">=3",
    "node": ">=8"
  },
  "bin": {
    "ev": "commands/ev.js"
  },
  "scripts": {
    "lint": "eslint **/*.js",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "circular-dep-check": "madge -c ./commands/ev.js"
  },
  "devDependencies": {
    "adm-zip": "0.4.13",
    "eslint": "6.7.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "jest": "25.0.0",
    "madge": "3.6.0"
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "ansi-regex": "5.0.0",
    "color-support": "1.1.3",
    "commander": "4.0.1",
    "dictionary-en-us": "2.1.1",
    "fs-extra": "8.1.0",
    "inquirer": "7.0.1",
    "moment": "2.24.0",
    "nspell": "2.1.2",
    "shelljs": "0.8.3"
  },
  "jest": {
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GetchaDEAGLE/eagle-versioner.git"
  },
  "author": "Daniel Eagle",
  "license": "MIT"
}
