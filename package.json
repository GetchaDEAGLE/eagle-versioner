{
  "name": "eagle-versioner",
  "version": "1.3.2",
  "description": "From the creation of special commits, calculates the semantic version based on the Git branch and commit history.",
  "engines": {
    "npm": ">=3",
    "node": ">=10"
  },
  "bin": {
    "ev": "commands/ev.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "circular-dep-check": "madge -c ./commands/ev.js"
  },
  "devDependencies": {
    "adm-zip": "0.4.16",
    "eslint": "7.10.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.5.2",
    "madge": "3.11.0"
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "ansi-regex": "5.0.0",
    "color-support": "1.1.3",
    "commander": "6.1.0",
    "dictionary-en": "3.0.1",
    "fs-extra": "9.0.1",
    "inquirer": "7.3.3",
    "moment": "2.29.1",
    "nspell": "2.1.2",
    "shelljs": "0.8.4"
  },
  "jest": {
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GetchaDEAGLE/eagle-versioner.git"
  },
  "keywords": [
    "semantic",
    "versioning",
    "changelog",
    "version",
    "wip",
    "commit",
    "calculate"
  ],
  "author": "Daniel Eagle",
  "license": "MIT"
}
